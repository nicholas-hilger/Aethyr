if(player.sprite_index != s_chosen and place_meeting(x,y,player) and!instance_exists(bossPar) and keyboard_check_pressed(vk_shift))
{
saveGame()
with(player)
{
if(player.area = 1) area = 2
else if(player.area = 2) area = 3
else if(player.area = 3) area = 1
motion_set(0,0)
